def int color(list[str] x,list[str] y,list[str] z){
	
	int i;
	for (i = 0; i<length(x); i = i+1){
		createElement("div","column",x[i]);
		createElement("div","column",y[i]);
		createElement("div","row",z[i]);
	}
	return 0;
}

